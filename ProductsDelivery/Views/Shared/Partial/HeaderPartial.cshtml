<!-- .....:::::: Start Header Section - Dark Header :::::.... -->
<header class="header-section dark-bg sticky-header d-none d-lg-block section-fluid-270" style="background: #70C951;">
    <div class="header-wrapper pos-relative">
        <div class="container-fluid">
            <div class="row justify-content-between align-items-center">
                <div class="col-auto">
                    <!-- Start Header Logo -->
                    <a asp-action="Index" asp-controller="Home" class="header-logo">
                      <img class="img-fluid" src="~/images/icons/fast-delivery-64.png" alt="" id="logo"><label  style="color:#fff; font-size:24px; margin-left: 10px;">Products Delivery</label>
                    </a>
                    <!-- End Header Logo -->
                </div>
                <div class="col-auto">
                    <div class="header-action">
                         
                    </div>
                </div>
                <div class="col-auto">
                    <div class="header-action">
                        <a asp-action="Cart" asp-controller="Home" class="header-action-item header-action-wishlist"><img src="~/images/icons/shopping-cart.png" alt=""></a>
                        <a asp-action="Index" asp-controller="Profile" class="header-action-item header-action-wishlist"><img src="~/images/icons/user_new1.png" alt=""></a>
                        <a asp-action="Logout" asp-controller="Person" class="header-action-item header-action-wishlist"><img src="~/images/icons/exit_new1.png" alt=""></a>
                    </div>
                </div>
            </div>
        </div>
    </div> 

</header>

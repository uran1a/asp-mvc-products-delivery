<!DOCTYPE HTML>
<html>
	<head>
		<title>Сборщик</title>
		@await Html.PartialAsync("Partial/MetatagsPartial")
		@await Html.PartialAsync("Partial/CssPartial")
		@await Html.PartialAsync("Partial/AdminCssPartial")
	</head>
	<body style="overflow-x:hidden;">
		
		<!-- Main -->
		<div class="container-fluid" style="padding: 0; margin: 0;">
			<div class="row">
				<div class="col-sm-12 col-md-3 col-lg-3">
					<div class="dashboard_tab_button">
						@await Html.PartialAsync("Sidebar")
					</div>
				</div>
				<div class="col-sm-12 col-md-9 col-lg-9 mb-50">
					@RenderBody()
				</div>
			</div>
		</div>
		<!-- Scripts -->
		@await Html.PartialAsync("Partial/ScriptsPartial")
		@await Html.PartialAsync("Partial/AdminScriptsPartial")
	</body>
</html>